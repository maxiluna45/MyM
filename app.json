{
  "expo": {
    "name": "M&M - Nuestra App",
    "slug": "MyM",
    "version": "1.0.0",
    "web": {
      "favicon": "./src/assets/icon.png",
      "name": "M&M - Nuestra App",
      "shortName": "M&M",
      "description": "Nuestra aplicaci√≥n para recordar momentos especiales",
      "themeColor": "#B2B0E8",
      "backgroundColor": "#ffffff",
      "display": "standalone",
      "orientation": "portrait",
      "startUrl": ".",
      "scope": "."
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": ["expo-dev-client"],
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#B2B0E8"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tuapp.mym",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#B2B0E8"
      },
      "package": "com.tuapp.mym"
    },
    "extra": {
      "eas": {
        "projectId": "113d29cb-5223-4266-99de-fabc5140e0cd"
      }
    }
  }
}
